{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "redirects": [
        { "source": "/", "destination": "/petopia", "permanent": true }
    ],
    "rewrites": [
        { "source": "/petopia", "destination": "/petopia/index.html" },
        { "source": "/petopia/(.*)", "destination": "/petopia/index.html" }
    ]
}
  